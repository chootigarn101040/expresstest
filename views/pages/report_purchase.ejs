<% include ../partials/header %>
<style type="text/css">
    body {
        background-color:pink;
        background-repeat: no-repeat;
        background-size: cover;
    }
</style>
<h1><center>Top 25 Purchases</center></h1>
<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Price</th>
            <th>Address</th>
           
        </tr>
    </thead>
    <tbody id="myTable">
    <% report.forEach(function(report){ %>
        <tr>
            <td scope="row"><%=report.name%></td>
            <td><%=report.price%></td>
            <td><%=report.address%></td>
           
        </tr>
        <% }); %>
    </tbody>
</table>



<% include ../partials/footer %>