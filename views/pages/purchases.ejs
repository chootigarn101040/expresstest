<% include ../partials/header %>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Price</th>

            <th><a  class="btn btn-success"  href="/addnewpurchases" role="button">Add New </a></th>
            
        </tr>
    </thead>
    <tbody>
    <% purchases.forEach(function(purchas){ %>
        <tr>
            <td scope="row"><%= purchas.id%></td>
            <td><%= purchas.title%></td>
            <td><%= purchas.price%></td>
            <td>
                <a name="" id="" class="btn btn-warning" href="/purchases/<%= purchas.id %>" role="button" >Edit</a> 

            </td>
        </tr>
        <% }); %>
    </tbody>
</table>

<% include ../partials/footer %>