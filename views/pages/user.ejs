<% include ../partials/header %>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>E-mail</th>
            <th>Password</th>
            <th><a  class="btn btn-success"  href="/addnewuser" role="button">Add User </a></th>
        </tr>
    </thead>
    <tbody>
    <% users.forEach(function(user){ %>
        <tr>
            <td scope="row"><%= user.id%></td>
            <td><%= user.email%></td>
            <td><%= user.password%></td>
            <td>
                <a name="" id="" class="btn btn-warning" href="/user/<%= user.id %>" role="button" >Edit</a>
            </td>
        </tr>
        <% }); %>
    </tbody>
</table>

<% include ../partials/footer %>