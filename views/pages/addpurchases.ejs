<% include ../partials/header %>

    <form action="/purchases/addnewpurchases" method="post">
        
        <div class="form-group">
            <label for="">Name</label>
            <input type="text" class="form-control"  name="name" id="" aria-describedby="helpId" placeholder="">
            <small id="helpId" class="form-text text-muted">Help Text</small>
        </div>

        <div class="form-group">
            <label for="">Address</label>
            <input type="text" class="form-control"  name="address" id="" aria-describedby="helpId" placeholder="">
            <small id="helpId" class="form-text text-muted">Help Text</small>
        </div>
        <div class="form-group">
            <label for="">State</label>
            <input type="text" class="form-control"  name="state" id="" aria-describedby="helpId" placeholder="">
            <small id="helpId" class="form-text text-muted">Help Text</small>
        </div>
        <div class="form-group">
            <label for="">Zipcode</label>
            <input type="number" class="form-control"  name="zipcode" id="" aria-describedby="helpId" placeholder="">
            <small id="helpId" class="form-text text-muted">Help Text</small>
        </div>
        <div class="form-group">
            <label for="">User_ID</label>
            <input type="number" class="form-control"  name="user_id" id="" aria-describedby="helpId" placeholder="">
            <small id="helpId" class="form-text text-muted">Help Text</small>
        </div>
        

        <button type="submit" class="btn btn-success">Add New</button>

    </form>

    <% include ../partials/footer %>